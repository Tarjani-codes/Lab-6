        "from operator import itemgetter\n",
        "\n",
        "food_items = [(\"Subway\", 220), (\"Coffee\", 100), (\"Whey\", 3000), (\"Beer\", 1000)]\n",
        "sorted_items = sorted(food_items, key=itemgetter(1), reverse=True)\n",
        "\n",
        "print(sorted_items)\n"
