      "source": [
        "from datetime import date\n",
        "\n",
        "date1 = (19, 9, 2023)\n",
        "date2 = (28, 9, 2023)\n",
        "d1 = date(*reversed(date1))\n",
        "d2 = date(*reversed(date2))\n",
        "days_between = abs((d2 - d1).days)\n",
        "print(\"Days between:\", days_between)\n"
      ]
    }
